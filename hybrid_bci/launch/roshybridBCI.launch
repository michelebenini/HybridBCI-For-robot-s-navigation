<launch>

	<node pkg="cnbiros_bci" type="rostid" name="rostid_set" output="screen">
       <rosparam>
         loopip: 127.0.0.1:8123
         pipe: /bus
         mode: SetOnly
         reconnect: 5.0
       </rosparam>
	</node>
	<node pkg="cnbiros_bci" type="rostid" name="rostid_get" output="screen">
       <rosparam>
         loopip: 127.0.0.1:8123
         pipe: /bus
         mode: GetOnly
         reconnect: 5.0
        </rosparam>
	</node>
	<node pkg="cnbiros_bci" type="rostic" name="rostic_get_p300" output="screen">
       <rosparam>
         loopip: 127.0.0.1:8123
         pipe: /ctrl0
         mode: GetOnly
         reconnect: 5.0
       </rosparam>
	</node>

  <node pkg="cnbiros_bci" type="rostic" name="rostic_get_smr" output="screen">
       <rosparam>
         loopip: 127.0.0.1:8123
         pipe: /ctrl1
         mode: GetOnly
         reconnect: 5.0
       </rosparam>
  </node>

</launch>
